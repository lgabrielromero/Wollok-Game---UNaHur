import wollok.game.*
import nivel1.*
import personajes.*

program juego_aventura {

	game.title("Skeleton Nightmare")
	game.height(10)
	game.width(11)
	game.cellSize(50)
	game.boardGround("fondoBlanco.png")

	nivelBloques.configurate()	
	player.movimientos()
	game.onTick(2000, "movimientoRandom", { craneo1.mover() })
	game.onTick(2000, "movimientoRandom", { craneo2.mover() })
	game.whenCollideDo(player, { elemento => 
		player.colision(elemento)
	})
	game.start()
}